  <method CLSNAME="ZCL_FI_RESETCLRDOC" CMPNAME="APPROVED" VERSION="1" LANGU="E" DESCRIPT="Approved event handler" EXPOSURE="2" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_FI_RESETCLRDOC" CMPNAME="APPROVED" SCONAME="IW_WFDAT" VERSION="1" LANGU="E" DESCRIPT="Structure for Workflow Approval" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="1" TYPE="ZCA_ST_WFAPRV"/>
   <parameter CLSNAME="ZCL_FI_RESETCLRDOC" CMPNAME="APPROVED" SCONAME="ET_RETURN" VERSION="1" LANGU="E" DESCRIPT="Return table" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="1" PARPASSTYP="0" TYPTYPE="1" TYPE="BAPIRET2_T"/>
   <source>method APPROVED.
     DATA: lv_bukrs TYPE zfidt001-bukrs
           , lv_augbl TYPE zfidt001-augbl
           , lv_auggj TYPE zfidt001-auggj.
    CLEAR et_return.

    lv_bukrs = iw_wfdat-objky+0(4).
    lv_augbl = iw_wfdat-objky+4(10).
    lv_auggj = iw_wfdat-objky+14(4).
    SELECT bukrs, augbl, auggj
      FROM zfidt001
      INTO TABLE @DATA(lt_data)
      WHERE bukrs = @lv_bukrs
      AND augbl = @lv_augbl
      AND auggj = @lv_auggj.

    IF lt_data IS INITIAL.
      RETURN.
    ENDIF.

    LOOP AT lt_data INTO DATA(ls_data).
      do_fbra(
        EXPORTING
          iv_bukrs  = ls_data-bukrs
          iv_augbl  = ls_data-augbl
          iv_auggj  = ls_data-auggj

        IMPORTING
          et_return = DATA(lt_return)
      ).

      APPEND LINES OF lt_return TO et_return.
    ENDLOOP.
  endmethod.</source>
  </method>
