   <parameter CLSNAME="ZCL_FI_PAYMENT_PROP" CMPNAME="BDC" SCONAME="ET_RETURN" VERSION="1" LANGU="E" DESCRIPT="Return table" CMPTYPE="1" MTDTYPE="0" EDITORDER="4 " DISPID="0 " PARDECLTYP="1" PARPASSTYP="0" TYPTYPE="1" TYPE="BAPIRET2_T"/>
   <source>METHOD BDC.
    DATA: lw_return     TYPE bapiret2.
      CLEAR lw_return.
      lw_return-type    = &apos;S&apos;.
      lw_return-message = &apos;Payment Proposal event request has been fully approved&apos;.
      APPEND lw_return TO et_return.
  ENDMETHOD.</source>
