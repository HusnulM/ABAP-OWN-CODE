  <method CLSNAME="ZCL_FI_CANCELIVDOC" CMPNAME="APPROVED" VERSION="1" LANGU="E" DESCRIPT="Approved" EXPOSURE="2" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_FI_CANCELIVDOC" CMPNAME="APPROVED" SCONAME="IW_WFDAT" VERSION="1" LANGU="E" DESCRIPT="Structure for Workflow Approval" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="1" TYPE="ZCA_ST_WFAPRV"/>
   <parameter CLSNAME="ZCL_FI_CANCELIVDOC" CMPNAME="APPROVED" SCONAME="ET_RETURN" VERSION="1" LANGU="E" DESCRIPT="Return table" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="1" PARPASSTYP="0" TYPTYPE="1" TYPE="BAPIRET2_T"/>
   <source>METHOD APPROVED.
    DATA: lt_data TYPE STANDARD TABLE OF zfidt003,
          lw_data TYPE zfidt003,
          lv_belnr  TYPE zfidt003-belnr,
          lv_gjahr  TYPE zfidt003-gjahr.

    lv_belnr = iw_wfdat-objky+0(10).
    lv_gjahr = iw_wfdat-objky+10(4).

    CLEAR:  lt_data[],
            et_return[].
    SELECT *
      FROM zfidt003
      INTO TABLE lt_data
***      WHERE reqno EQ iw_wfdat-objky.
      WHERE belnr EQ lv_belnr
        AND gjahr EQ lv_gjahr.

    IF lt_data[] IS INITIAL.
      RETURN.
    ENDIF.

    LOOP AT lt_data INTO lw_data.
      bdc_mr8m(
        EXPORTING
          iv_belnr  = lw_data-belnr
          iv_gjahr  = lw_data-gjahr
          iv_stgrd  = lw_data-stgrd
          iv_budat  = lw_data-budat
          iv_test   = lw_data-test_mode
        IMPORTING
          et_return = DATA(lt_return)
      ).

      APPEND LINES OF lt_return TO et_return.
    ENDLOOP.
  ENDMETHOD.</source>
  </method>
