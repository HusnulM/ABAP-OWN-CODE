  <method CLSNAME="ZCL_FI_PAYMENT_PROP" CMPNAME="APPROVED" VERSION="1" LANGU="E" DESCRIPT="Approved" EXPOSURE="2" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_FI_PAYMENT_PROP" CMPNAME="APPROVED" SCONAME="IW_WFDAT" VERSION="1" LANGU="E" DESCRIPT="Structure for Workflow Approval" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="1" TYPE="ZCA_ST_WFAPRV"/>
   <parameter CLSNAME="ZCL_FI_PAYMENT_PROP" CMPNAME="APPROVED" SCONAME="ET_RETURN" VERSION="1" LANGU="E" DESCRIPT="Return table" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="1" PARPASSTYP="0" TYPTYPE="1" TYPE="BAPIRET2_T"/>
   <source>METHOD APPROVED.
    DATA: lt_data TYPE STANDARD TABLE OF zfidt004,
          lw_data TYPE zfidt004,
          lv_belnr  TYPE zfidt004-belnr,
          lv_gjahr  TYPE zfidt004-gjahr,
          lv_bukrs  TYPE zfidt004-bukrs.

    lv_bukrs = iw_wfdat-objky+0(4).
    lv_belnr = iw_wfdat-objky+4(10).
    lv_gjahr = iw_wfdat-objky+14(4).

    CLEAR:  lt_data[],
            et_return[].
    SELECT *
      FROM zfidt004
      INTO TABLE lt_data
      WHERE bukrs EQ lv_bukrs
        AND belnr EQ lv_belnr
        AND gjahr EQ lv_gjahr.

    IF lt_data[] IS INITIAL.
      RETURN.
    ENDIF.

    LOOP AT lt_data INTO lw_data.
      bdc(
        EXPORTING
          iv_belnr  = lw_data-belnr
          iv_gjahr  = lw_data-gjahr
          iv_bukrs  = lw_data-bukrs
        IMPORTING
          et_return = DATA(lt_return)
      ).

      APPEND LINES OF lt_return TO et_return.
    ENDLOOP.
  ENDMETHOD.</source>
  </method>
