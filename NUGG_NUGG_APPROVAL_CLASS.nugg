  <method CLSNAME="ZCL_FI_REVERSALDOC" CMPNAME="APPROVED" VERSION="1" LANGU="E" DESCRIPT="Approved" EXPOSURE="2" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_FI_REVERSALDOC" CMPNAME="APPROVED" SCONAME="IW_WFDAT" VERSION="1" LANGU="E" DESCRIPT="Structure for Workflow Approval" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="1" TYPE="ZCA_ST_WFAPRV"/>
   <parameter CLSNAME="ZCL_FI_REVERSALDOC" CMPNAME="APPROVED" SCONAME="ET_RETURN" VERSION="1" LANGU="E" DESCRIPT="Return table" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="1" PARPASSTYP="0" TYPTYPE="1" TYPE="BAPIRET2_T"/>
   <source>METHOD approved.
    CLEAR et_return.

    SELECT bukrs, belnr, gjahr, stgrd, budat, monat, vatdate, voidr
      FROM zfidt002
      INTO TABLE @DATA(lt_data)
      WHERE reqno EQ @iw_wfdat-objky.

    IF lt_data IS INITIAL.
      RETURN.
    ENDIF.

    LOOP AT lt_data INTO DATA(ls_data).
      bdc_fb08(
        EXPORTING
          iv_belnr  = ls_data-belnr
          iv_bukrs  = ls_data-bukrs
          iv_gjahr  = ls_data-gjahr
          iv_stgrd  = ls_data-stgrd
          iv_budat  = ls_data-budat
          iv_vatdt  = ls_data-vatdate
          iv_monat  = ls_data-monat
          iv_voidr  = ls_data-voidr
        IMPORTING
          et_return = DATA(lt_return)
      ).

      APPEND LINES OF lt_return TO et_return.
    ENDLOOP.
  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_FI_REVERSALDOC" CMPNAME="BDC_FB08" VERSION="1" LANGU="E" DESCRIPT="Call Transaction FB08" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_FI_REVERSALDOC" CMPNAME="BDC_FB08" SCONAME="IV_BELNR" VERSION="1" LANGU="E" DESCRIPT="Document Number of an Accounting Document" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="1" TYPE="BELNR_D"/>
   <parameter CLSNAME="ZCL_FI_REVERSALDOC" CMPNAME="BDC_FB08" SCONAME="IV_BUKRS" VERSION="1" LANGU="E" DESCRIPT="Company Code" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="1" TYPE="BUKRS"/>
   <parameter CLSNAME="ZCL_FI_REVERSALDOC" CMPNAME="BDC_FB08" SCONAME="IV_GJAHR" VERSION="1" LANGU="E" DESCRIPT="Fiscal Year" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="1" TYPE="GJAHR"/>
   <parameter CLSNAME="ZCL_FI_REVERSALDOC" CMPNAME="BDC_FB08" SCONAME="IV_STGRD" VERSION="1" LANGU="E" DESCRIPT="Reason for Reversal or Inverse Posting" CMPTYPE="1" MTDTYPE="0" EDITORDER="4 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="1" TYPE="STGRD"/>
   <parameter CLSNAME="ZCL_FI_REVERSALDOC" CMPNAME="BDC_FB08" SCONAME="IV_BUDAT" VERSION="1" LANGU="E" DESCRIPT="Posting Date in the Document" CMPTYPE="1" MTDTYPE="0" EDITORDER="5 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="1" TYPE="BUDAT" PAROPTIONL="X"/>
   <parameter CLSNAME="ZCL_FI_REVERSALDOC" CMPNAME="BDC_FB08" SCONAME="IV_VATDT" VERSION="1" LANGU="E" DESCRIPT="Tax Reporting Date" CMPTYPE="1" MTDTYPE="0" EDITORDER="6 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="1" TYPE="VATDATE" PAROPTIONL="X"/>
   <parameter CLSNAME="ZCL_FI_REVERSALDOC" CMPNAME="BDC_FB08" SCONAME="IV_MONAT" VERSION="1" LANGU="E" DESCRIPT="Fiscal period" CMPTYPE="1" MTDTYPE="0" EDITORDER="7 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="1" TYPE="MONAT" PAROPTIONL="X"/>
   <parameter CLSNAME="ZCL_FI_REVERSALDOC" CMPNAME="BDC_FB08" SCONAME="IV_VOIDR" VERSION="1" LANGU="E" DESCRIPT="Check void reason code" CMPTYPE="1" MTDTYPE="0" EDITORDER="8 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="1" TYPE="VOIDR" PAROPTIONL="X"/>
   <parameter CLSNAME="ZCL_FI_REVERSALDOC" CMPNAME="BDC_FB08" SCONAME="ET_RETURN" VERSION="1" LANGU="E" DESCRIPT="Return table" CMPTYPE="1" MTDTYPE="0" EDITORDER="9 " DISPID="0 " PARDECLTYP="1" PARPASSTYP="0" TYPTYPE="1" TYPE="BAPIRET2_T"/>
   <source>METHOD bdc_fb08.
    DATA:
      lv_mode    TYPE char1,
      lv_budat   TYPE char10,
      lv_vatdt   TYPE char10,
      lv_voidr   TYPE char2,
      lt_bdcdata TYPE bdcdata_tab,
      lt_bdcmsg  TYPE tab_bdcmsgcoll.

    IF iv_budat EQ &apos;00000000&apos;.
      lv_budat = &apos;&apos;.
    ELSE.
      WRITE iv_budat TO lv_budat.
    ENDIF.

    IF iv_vatdt EQ &apos;00000000&apos;.
      lv_vatdt = &apos;&apos;.
    ELSE.
      WRITE iv_vatdt TO lv_vatdt.
    ENDIF.

    IF iv_voidr IS INITIAL.
      lv_voidr = &apos;&apos;.
    ELSE.
      lv_voidr = iv_voidr.
    ENDIF.

    lt_bdcdata = VALUE #(
      ( program = &apos;SAPMF05A&apos; dynpro = &apos;0105&apos; dynbegin = &apos;X&apos; )
          ( fnam = &apos;BDC_OKCODE&apos;     fval = &apos;=BU&apos; )
          ( fnam = &apos;RF05A-BELNS&apos;    fval = iv_belnr )
          ( fnam = &apos;BKPF-BUKRS&apos;     fval = iv_bukrs )
          ( fnam = &apos;RF05A-GJAHS&apos;    fval = iv_gjahr )
          ( fnam = &apos;UF05A-STGRD&apos;    fval = iv_stgrd )
          ( fnam = &apos;BSIS-BUDAT&apos;     fval = lv_budat )
          ( fnam = &apos;VBKPF-VATDATE&apos;  fval = lv_vatdt )
          ( fnam = &apos;BSIS-MONAT&apos;     fval = iv_monat )
          ( fnam = &apos;RF05A-VOIDR&apos;    fval = lv_voidr )
    ).

    lv_mode = &apos;N&apos;.
    CALL TRANSACTION &apos;FB08&apos; USING lt_bdcdata
                            MODE lv_mode
                            MESSAGES INTO lt_bdcmsg.

    CLEAR et_return.

    LOOP AT lt_bdcmsg INTO DATA(ls_bdcmsg).
      APPEND INITIAL LINE TO et_return ASSIGNING FIELD-SYMBOL(&lt;fs_return&gt;).
      &lt;fs_return&gt;-type       = ls_bdcmsg-msgtyp.
      &lt;fs_return&gt;-id         = ls_bdcmsg-msgid.
      &lt;fs_return&gt;-number     = ls_bdcmsg-msgnr.
      &lt;fs_return&gt;-message_v1 = ls_bdcmsg-msgv1.
      &lt;fs_return&gt;-message_v2 = ls_bdcmsg-msgv2.
      &lt;fs_return&gt;-message_v3 = ls_bdcmsg-msgv3.
      &lt;fs_return&gt;-message_v4 = ls_bdcmsg-msgv4.
      MESSAGE ID &lt;fs_return&gt;-id TYPE &lt;fs_return&gt;-type NUMBER &lt;fs_return&gt;-number
      WITH &lt;fs_return&gt;-message_v1 &lt;fs_return&gt;-message_v2 &lt;fs_return&gt;-message_v3 &lt;fs_return&gt;-message_v4 INTO &lt;fs_return&gt;-message.
    ENDLOOP.
  ENDMETHOD.</source>
  </method>
