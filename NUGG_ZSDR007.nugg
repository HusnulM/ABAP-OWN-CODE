ENDFORM.

FORM user_command USING r_ucomm LIKE sy-ucomm
      rs_selfield TYPE slis_selfield.

  DATA : lw_final TYPE zds_sdr007,
         ref_grid TYPE REF TO cl_gui_alv_grid.


  &quot;TO capture the remark IN the final internal TABLE gt_final â€œ
  CALL FUNCTION &apos;GET_GLOBALS_FROM_SLVC_FULLSCR&apos;
    IMPORTING
      e_grid = ref_grid.

  IF ref_grid IS NOT INITIAL.
    CALL METHOD ref_grid-&gt;check_changed_data( ).
  ENDIF.

  CASE r_ucomm.
    WHEN &apos;&amp;IC1&apos;.
      CLEAR ls_data.
      READ TABLE lt_data INTO ls_data INDEX rs_selfield-tabindex.
      CASE rs_selfield-fieldname.
        WHEN &apos;VBELN&apos;.
          IF ls_data-vbeln IS NOT INITIAL.
            SET PARAMETER ID &apos;VF&apos; FIELD ls_data-vbeln.
            CALL TRANSACTION &apos;VF03&apos; AND SKIP FIRST SCREEN.
          ENDIF.

        WHEN &apos;ZPENBIL&apos;.
          IF ls_data-zpenbil IS NOT INITIAL.
            SET PARAMETER ID &apos;VF&apos; FIELD ls_data-zpenbil.
            CALL TRANSACTION &apos;VF03&apos; AND SKIP FIRST SCREEN.
          ENDIF.
      ENDCASE.
    WHEN &apos;&amp;ZBILL&apos;.
      DATA is_err TYPE char1.
      CLEAR is_err.
      &quot;Validasi
      LOOP AT lt_data INTO ls_data
      WHERE zpenbil IS INITIAL AND clrst = &apos;C&apos; AND pnlty &gt; 0.
        IF ls_data-zinvnum IS INITIAL OR ls_data-zinvrem IS INITIAL.
          IF ls_data-zinvnum IS INITIAL.
            ls_data-zsnote = &apos;Please fill Invoice Reff&apos;.
          ENDIF.

          IF ls_data-zinvrem IS INITIAL.
            ls_data-zsnote = &apos;Please fill Invoice Remark&apos;.
          ENDIF.

          MODIFY lt_data FROM ls_data
          TRANSPORTING zsnote
          WHERE vbeln = ls_data-vbeln.

          is_err = &apos;X&apos;.
        ENDIF.

        CLEAR ls_data.
      ENDLOOP.

      IF is_err IS INITIAL.
        DATA : ld_answer TYPE string.
        CLEAR ld_answer.
        CALL FUNCTION &apos;POPUP_CONTINUE_YES_NO&apos;
          EXPORTING
            textline1 = &apos;Continue to Generate Billing Document?&apos;
            textline2 = &apos;Yes / No?!&apos;
            titel     = &apos;Generate Billing Confirmation&apos;
          IMPORTING
            answer    = ld_answer.
        IF ld_answer = &apos;J&apos;.
          DATA(xdata) = lt_data.
          DELETE xdata WHERE clrst   &lt;&gt; &apos;C&apos;.
          DELETE xdata WHERE zpenbil IS NOT INITIAL.
          DELETE xdata WHERE pnlty   IS INITIAL.

          IF xdata IS NOT INITIAL.
            IF is_err IS INITIAL.
              PERFORM generate_billing.
            ENDIF.

            rs_selfield-refresh = abap_true.
          ELSE.
            MESSAGE &apos;No data processed&apos; TYPE &apos;S&apos; DISPLAY LIKE &apos;E&apos;.
          ENDIF.
          REFRESH xdata.
        ENDIF.
      ELSE.
        MESSAGE &apos;Please check mandatory input!&apos; TYPE &apos;S&apos; DISPLAY LIKE &apos;E&apos;.
        rs_selfield-refresh = abap_true.
      ENDIF.

  ENDCASE.
ENDFORM.

FORM data_changed ##called
     USING lrc_i_dc TYPE REF TO cl_alv_changed_data_protocol.

*  BREAK sap_husnul.

  DATA: &quot;TODO: Change the type of ITAB
    lit_alv_new TYPE STANDARD TABLE OF zds_sdr007.

  ASSIGN lrc_i_dc-&gt;mp_mod_rows-&gt;* TO FIELD-SYMBOL(&lt;fs_rows&gt;).
  IF sy-subrc = 0.
    lit_alv_new[] = &lt;fs_rows&gt;.

    READ TABLE lit_alv_new INTO DATA(ls_alv_data) INDEX 1.
    IF ls_alv_data-zpenbil IS INITIAL AND ls_alv_data-clrst = &apos;C&apos;.
      IF ls_alv_data-zinvnum IS INITIAL.
        MESSAGE &apos;Please fiil invoice reff&apos; TYPE &apos;E&apos;.
      ELSEIF ls_alv_data-zinvrem IS INITIAL.
        MESSAGE &apos;Please fiil invoice remark&apos; TYPE &apos;E&apos;.
      ENDIF.
    ENDIF.
  ENDIF.
ENDFORM.</source>
