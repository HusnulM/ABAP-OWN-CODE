FORM fill_eventstab TABLES p_eventstab STRUCTURE s_events.
  CALL FUNCTION &apos;REUSE_ALV_EVENTS_GET&apos;
    EXPORTING
      i_list_type     = 0
    IMPORTING
      et_events       = p_eventstab[]
    EXCEPTIONS
      list_type_wrong = 1
