 <PROG NAME="ZSDR007_F03" VARCL="X" SUBC="I" RMAND="400" RLOAD="E" UCCHECK="X">
  <textPool>
   <language SPRAS="E">
    <textElement ID="R" ENTRY="Include ZSDR007_F03" LENGTH="19 "/>
   </language>
  </textPool>
  <source>*&amp;---------------------------------------------------------------------*
*&amp; Include          ZSDR007_F03
*&amp;---------------------------------------------------------------------*

FORM generate_billing.
  CLEAR ls_data.

  LOOP AT lt_data INTO ls_data
  WHERE zpenbil IS INITIAL AND clrst = &apos;C&apos; AND pnlty &gt; 0.

    PERFORM call_bapi.

    CLEAR ls_data.
  ENDLOOP.

  MESSAGE &apos;Billing document creation process is complete, please check Note for details&apos; TYPE &apos;S&apos; DISPLAY LIKE &apos;S&apos;.
ENDFORM.

FORM call_bapi.
  CLEAR: creatordatain, lv_matnr_penalty.

  SELECT SINGLE low FROM tvarvc
    INTO lv_matnr_penalty
      WHERE name = &apos;ZSD_MAT_ZSDR007_DENDA&apos;.

  lv_matnr_penalty = |{ lv_matnr_penalty ALPHA = IN }|.

  SELECT SINGLE * FROM vbrk
    INTO @DATA(ls_vbrk)
        WHERE vbeln = @ls_data-vbeln.

  REFRESH lt_billingdatain.
  CLEAR ls_billingdatain.
  ls_billingdatain-salesorg   = ls_vbrk-vkorg.
  ls_billingdatain-distr_chan = ls_vbrk-vtweg.
  ls_billingdatain-division   = ls_vbrk-spart.
  CALL FUNCTION &apos;CONVERSION_EXIT_AUART_INPUT&apos;
    EXPORTING
      input  = &apos;OR&apos;
    IMPORTING
      output = ls_billingdatain-doc_type.
*  ls_billingdatain-doc_type   = &apos;OR&apos;.
  ls_billingdatain-ordbilltyp = &apos;ZIEB&apos;.
  ls_billingdatain-bill_date  = sy-datum.
  ls_billingdatain-sold_to    = ls_data-kunnr.
  ls_billingdatain-item_categ = &apos;ZIEB&apos;.
  SELECT SINGLE ktgrm
    FROM mvke INTO ls_billingdatain-acctasgnmt
      WHERE matnr = lv_matnr_penalty.
  ls_billingdatain-price_date = sy-datum.
  SELECT SINGLE werks FROM vbrp
    INTO ls_billingdatain-plant
      WHERE vbeln = ls_data-vbeln.
  ls_billingdatain-bill_to    = ls_data-kunnr.
  ls_billingdatain-payer      = ls_data-kunnr.
  ls_billingdatain-ship_to    = ls_data-kunnr.
  ls_billingdatain-ref_doc    = ls_data-vbeln.
  ls_billingdatain-material   = lv_matnr_penalty.
  ls_billingdatain-req_qty    = 1.
  ls_billingdatain-currency   = ls_data-waers.
  ls_billingdatain-ref_item   = &apos;000001&apos;.

  SELECT SINGLE prctr FROM marc
    INTO ls_billingdatain-profit_ctr
      WHERE matnr = lv_matnr_penalty AND werks = ls_billingdatain-plant.
  ls_billingdatain-itm_number = &apos;000001&apos;.

  SELECT SINGLE zterm FROM knvv
    INTO ls_billingdatain-payment_terms
      WHERE kunnr = ls_data-kunnr.
  APPEND ls_billingdatain TO lt_billingdatain.

  REFRESH lt_conditiondatain.
  CLEAR ls_conditiondatain.
  ls_conditiondatain-data_index = 1.
  ls_conditiondatain-cond_type  = &apos;ZPRT&apos;.
  ls_conditiondatain-cond_value = ls_data-pnlty.
  ls_conditiondatain-cond_curr  = ls_data-waers.
  ls_conditiondatain-cond_p_unt = 1.
  APPEND ls_conditiondatain TO lt_conditiondatain.

  REFRESH lt_textdatain.
  CLEAR ls_textdatain.
  ls_textdatain-applobject = &apos;VBBK&apos;.
  ls_textdatain-text_id    = &apos;ZB01&apos;.
  ls_textdatain-langu      = sy-langu.
  ls_textdatain-text_line  = ls_data-zinvnum.
  APPEND ls_textdatain TO lt_textdatain.

  CLEAR ls_textdatain.
  ls_textdatain-applobject = &apos;VBBK&apos;.
  ls_textdatain-text_id    = &apos;ZB00&apos;.
  ls_textdatain-langu      = sy-langu.
  ls_textdatain-text_line  = ls_data-zinvrem.
  APPEND ls_textdatain TO lt_textdatain.


  CALL FUNCTION &apos;BAPI_BILLINGDOC_CREATEMULTIPLE&apos;
    EXPORTING
      creatordatain   = creatordatain
      testrun         = testrun
      posting         = posting
    TABLES
      billingdatain   = lt_billingdatain
      conditiondatain = lt_conditiondatain
*     CCARDDATAIN     =
*     textdatain      = lt_textdatain
*     ERRORS          =
      return          = lt_return
      success         = lt_success
*     EXTENSIONIN     =
*     PARTNERDATAIN   =
*     NFMETALLITMS    =
    .

  READ TABLE lt_success INTO DATA(ls_result) INDEX 1.
  IF ls_result-bill_doc IS NOT INITIAL.
    ls_data-zpenbil = ls_result-bill_doc.
    ls_data-zsnote  = |Billing Denda Berhasil dibuat dengan nomor { ls_result-bill_doc }|.

    CLEAR ls_zsdt014.
*    ls_zsdt014-zsrcbil = ls_data-vbeln.
*    ls_zsdt014-zpenbil = ls_result-bill_doc.
    INSERT INTO zsdt014 VALUES @( VALUE #( mandt = sy-mandt zsrcbil = ls_data-vbeln zpenbil =  ls_result-bill_doc ) ).
*    MODIFY zsdt014 FROM ls_zsdt014.
    COMMIT WORK.

*    break sap_husnul.
    PERFORM create_text USING &apos;ZB01&apos; ls_result-bill_doc &apos;VBBK&apos; ls_data-zinvnum. &quot;Long Text Invoice Number
    PERFORM create_text USING &apos;ZB00&apos; ls_result-bill_doc &apos;VBBK&apos; ls_data-zinvrem. &quot;Long Text Remark
  ELSE.
    READ TABLE lt_return INTO DATA(ls_return) INDEX 1.
    ls_data-zsnote  = |{ ls_return-message } { ls_return-message_v1 }|.
  ENDIF.

  ls_data-matnr2 = lv_matnr_penalty.
  SELECT SINGLE maktx FROM makt
    INTO ls_data-maktx2
      WHERE matnr = lv_matnr_penalty AND spras = sy-langu.
  MODIFY lt_data FROM ls_data
    TRANSPORTING zpenbil zsnote matnr2 maktx2
      WHERE vbeln = ls_data-vbeln.

ENDFORM.

FORM create_text USING p_id p_name p_obj p_text.

  DATA:
    fid       TYPE thead-tdid,
    flanguage TYPE thead-tdspras,
    fname	    TYPE thead-tdname,
    fobject	  TYPE thead-tdobject,
    lt_flines	TYPE TABLE OF tline,
    ls_flines	TYPE tline.

  REFRESH lt_flines.
  CLEAR: fid, fname, fobject.
  fid     = p_id.
  fname   = p_name.
  fobject = p_obj.
  flanguage = sy-langu.

  CLEAR ls_flines.
  ls_flines-tdformat = &apos;*&apos;.
  ls_flines-tdline   = p_text.
  APPEND ls_flines TO lt_flines.

  CALL FUNCTION &apos;CREATE_TEXT&apos;
    EXPORTING
      fid       = fid
      flanguage = flanguage
      fname     = fname
      fobject   = fobject
    TABLES
      flines    = lt_flines
    EXCEPTIONS
      no_init   = 1
      no_save   = 2
      OTHERS    = 3.

  IF sy-subrc = 0.
    CALL FUNCTION &apos;BAPI_TRANSACTION_COMMIT&apos;
      EXPORTING
        wait = &apos;X&apos;.
  ENDIF.
ENDFORM.</source>
 </PROG>
